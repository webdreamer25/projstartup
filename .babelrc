{
  "presets": [
    [
      "@babel/preset-env", {
        "loose": true,
        "targets": {
          "node": "10.13",
          "browsers": ["last 2 versions", "ie >= 11"]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-syntax-dynamic-import"
  ]
}

